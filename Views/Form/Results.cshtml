@model DojoSurvey.Models.Info

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Dojo Survey Results</title>
    <link rel="stylesheet" href="~/css/style.css">
</head>
<body>
    <section class="results">
        <h3>Submitted Info:</h3>
        <p><span class="title">Name:</span><span>@Model.Name</span></p>
        <p><span class="title">Dojo Location:</span><span>@Model.Location</span></p>
        <p><span class="title">Favorite Language:</span><span>@Model.Language</span></p>
        <p><span class="title">Comments:</span><span>@Model.Comments</span></p>
        <a href="/">Go Back</a>
    </section>
</body>
</html>